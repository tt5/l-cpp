g++ -fmodules-ts -x c++-system-header iostream
g++ hello.cpp
g++ -std=c++20 -fmodules-ts module.cpp hello.cpp
g++ -std=c++20 -fmodules-ts module.cpp module_impl.cpp hello.cpp
g++ -std=c++20 -fmodules-ts -c -x c++-system-header iostream
g++ -std=c++20 -fmodules-ts hello.cpp module.cpp module_impl.cpp
